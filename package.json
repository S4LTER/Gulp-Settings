{
  "name": "joseffie-gulp-pack",
  "description": "Starter template and gulp settings for web development",
  "author": "Ivan Gavrilich (joseffie)",
  "license": "GPL-3.0-only",
  "version": "6.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/joseffie/gulp-joseffie-settings.git"
  },
  "main": "gulpfile.babel.js",
  "type": "module",
  "scripts": {
    "dev": "gulp",
    "build": "gulp build --production",
    "zip": "gulp zip --production",
    "cleanup": "gulp cleanup",
    "build:pug": "gulp pug --production",
    "build:styles": "gulp styles --production",
    "build:scripts": "gulp scripts --production",
    "build:images": "gulp images --production",
    "build:fonts": "gulp compileFonts && gulp fonts",
    "build:sprites": "gulp sprites",
    "deploy": "npm run build && gulp publish",
    "publish": "gulp publish",
    "lint:pug": "pug-lint src/",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:scripts": "eslint --ext .js,.cjs,.mjs src/**/*.js"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-function-sent": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "@prettier/plugin-pug": "^2.4.1",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^9.1.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "bem-tools-core": "^0.0.3",
    "bem-tools-create": "^2.3.0",
    "browser-sync": "^2.27.10",
    "chalk": "^5.2.0",
    "cssnano": "^5.1.13",
    "del": "^7.0.0",
    "emitty": "^1.4.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-fonter": "^0.3.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^8.0.0",
    "gulp-load-plugins": "^2.0.8",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-posthtml": "^3.0.5",
    "gulp-pug": "^5.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-rollup-each": "^4.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-svg-sprite": "^2.0.1",
    "gulp-ttf2woff2": "^4.0.1",
    "gulp-version-number": "^0.2.4",
    "gulp-webp": "^4.0.1",
    "gulp-webp-html-nosvg": "^1.1.1",
    "gulp-webpcss": "^1.1.1",
    "gulp-zip": "^5.1.0",
    "gulplog": "^2.0.0",
    "htmlnano": "^2.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.1.1",
    "moment": "^2.29.4",
    "postcss": "^8.4.16",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^8.0.1",
    "postcss-reporter": "^7.0.5",
    "postcss-sort-media-queries": "^4.3.0",
    "posthtml-attrs-sorter": "^1.1.0",
    "posthtml-load-config": "^2.0.0",
    "prettier": "^2.7.1",
    "pug-lint": "^2.6.0",
    "rollup": "^3.10.0",
    "sass": "^1.54.5",
    "stylelint": "^14.11.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "webp-converter": "2.2.3",
    "webp-in-css": "^0.8.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "attrs": "^1.1.0",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "object.omit": "^3.0.0",
    "slide-anim": "^1.3.2",
    "svg4everybody": "^2.1.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.pug": [
      "yarn prettier --write src/**/*.pug --config .prettierrc.json",
      "yarn pug-lint src/",
      "git add"
    ],
    "*.scss": [
      "yarn prettier --write src/**/*.scss --config .prettierrc.json",
      "yarn stylelint \"src/**/*.scss\" --fix",
      "git add"
    ],
    "*.{js,cjs}": [
      "yarn prettier --write src/**/*.{js,cjs} --config .prettierrc.json",
      "yarn eslint --ext .js,.cjs src/**/*.js --fix",
      "git add"
    ]
  }
}
